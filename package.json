{
  "name": "react-vr-graphql",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node -e \"console.log('open browser at http://localhost:8081/vr/\\n\\n');\" && node node_modules/react-native/local-cli/cli.js start",
    "bundle": "node node_modules/react-vr/scripts/bundle.js",
    "open": "node -e \"require('xopen')('http://localhost:8081/vr/')\"",
    "devtools": "react-devtools",
    "test": "jest",
    "deploy": "gh-pages -d vr/build"
  },
  "dependencies": {
    "apollo-client": "^1.2.2",
    "ovrui": "^1.1.0",
    "react": "~15.4.1",
    "react-apollo": "^1.2.0",
    "react-native": "~0.42.0",
    "react-vr": "^1.1.0",
    "react-vr-web": "^1.1.0",
    "three": "^0.80.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-preset-react-native": "^1.9.1",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^7.0.0",
    "gh-pages": "^1.0.0",
    "jest": "^19.0.2",
    "react-devtools": "^2.1.3",
    "react-test-renderer": "~15.4.1",
    "xopen": "1.0.0"
  },
  "jest": {
    "preset": "react-vr"
  }
}
